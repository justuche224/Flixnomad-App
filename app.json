{
  "expo": {
    "name": "flixnomad",
    "slug": "flixnomad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/android/play_store_512.png",
    "scheme": "flixnomad",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios/AppIcon~ios-marketing.png",
      "bundleIdentifier": "com.flixnomad.movies"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "android": {
      "icon": "./assets/icons/android/res/mipmap-xxxhdpi/flixnomad.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/res/mipmap-xxxhdpi/flixnomad_foreground.png",
        "monochromeImage": "./assets/icons/android/res/mipmap-xxxhdpi/flixnomad_monochrome.png",
        "backgroundImage": "./assets/icons/android/res/mipmap-xxxhdpi/flixnomad_background.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flixnomad.movies"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/web/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "08c53faa-2bad-4ed1-93e9-752f1e467347"
      }
    }
  }
}
